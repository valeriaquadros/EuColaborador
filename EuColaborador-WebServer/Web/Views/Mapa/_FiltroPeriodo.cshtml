<div class="modal fade" id="filtroPeriodoModal" tabindex="-1" role="dialog" aria-labelledby="filtroPeriodoModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="filtroPeriodoModalLabel">Insira o período para filtro</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div id="validationerror" class="text-danger"></div>
                    <div class="form-group">
                        <label for="recipient-name" class="col-form-label">Data inicial</label>
                        <input type="date" class="form-control" id="dataInicialFiltro" value="@Model.DataInicial.ToString("yyyy-MM-dd")">
                    </div>
                    <div class="form-group">
                        <label for="recipient-name" class="col-form-label">Data final</label>
                        <input type="date" class="form-control" id="dataFinalFiltro" value="@Model.DataFinal.ToString("yyyy-MM-dd")">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
                <button type="button" class="btn btn-primary" onclick="FiltrarMapa()">Filtrar</button>
            </div>
        </div>
    </div>
</div>